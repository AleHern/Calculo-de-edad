#include <stdio.h>
/*Determinar si es mayor de edad o menor de edad*/
int main()
{
	int n = 17;
	printf("ingresa tu edad:\n");
	scanf("%d",&n);
	if(n <= 17){
	   printf("eres menor de edad");
}
else{
	   printf ("eres mayor de edad");
}
return 0;
}
